{
    "family": "CLUSTER_NAME",
    "executionRoleArn": "arn:aws:iam::ACOUNT_ID:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "CLUSTER_NAME",
            "image": "ACOUNT_ID.dkr.ecr.REGION.amazonaws.com/CLUSTER_NAME:latest",
            "portMappings": [
                {
                    "containerPort": PORT,
                    "hostPort": PORT,
                    "protocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}